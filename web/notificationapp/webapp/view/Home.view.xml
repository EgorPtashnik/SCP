<mvc:View controllerName="notificationapp.controller.Home" displayBlock="true"
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:w="sap.ui.integration.widgets"
  xmlns:mvc="sap.ui.core.mvc">
  <Page showHeader="false">
    <f:GridContainer>
      <w:Card class="sapUiTinyMargin" manifest="./cards/notificationManifest.json">
        <w:layoutData>
          <f:GridContainerItemLayoutData columns="4"/>
        </w:layoutData>
      </w:Card>
    </f:GridContainer>
  </Page>
</mvc:View>
